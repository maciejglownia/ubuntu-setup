#set default speaker 
pactl list short sinks 
pactl set-default-sink bluez_sink.AC_80_0A_6D_38_B4.handsfree_head_unit

#set default microphone
pactl list short sorces 
pactl set-default-source bluez_source.AC_80_0A_6D_38_B4.handsfree_head_unit  

#sound settings
pavucontrol

#kill specific port
sudo netstat -plten |grep java
